	<queryString>
		<![CDATA[SELECT
	permintaan_non_medis.ruang,
	permintaan_non_medis.no_permintaan,
	permintaan_non_medis.tanggal,
	ipsrsbarang.nama_brng,
	detail_permintaan_non_medis.jumlah,
	kodesatuan.satuan,
	ipsrsbarang.harga,
	(ipsrsbarang.harga * detail_permintaan_non_medis.jumlah) as total	
FROM
	permintaan_non_medis,
	detail_permintaan_non_medis,
	ipsrsbarang,
	kodesatuan
WHERE
	permintaan_non_medis.no_permintaan= detail_permintaan_non_medis.no_permintaan 
	and ipsrsbarang.kode_brng=detail_permintaan_non_medis.kode_brng
	and kodesatuan.kode_sat=detail_permintaan_non_medis.kode_sat
	and permintaan_non_medis.ruang=$P{parameter}
	AND permintaan_non_medis.tanggal BETWEEN $P{tanggal1} and $P{tanggal2}
	order by permintaan_non_medis.tanggal]]>
	</queryString>
